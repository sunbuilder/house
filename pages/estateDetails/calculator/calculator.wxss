.calcuator{
  display:flex;
  justify-content: center;
  align-items: center;
  height:180px;
  width:100%;
}
.calcuator-left{
  height:180px;
  width:50%;
 justify-content: center;
}
.calcuator-right{
  height:180px;
  width:50%; 
}
.calcuator-title{
  display:flex;
  justify-content: center;
  align-items: center;
  height:60px;
  width:100%;
}
.calcuator-title-in{
    height:35px;
     width:60%;
     background-color:white;
     color:#F44E15;
     font-size:30rpx;
     display:flex;
     justify-content: center;
     align-items: center;
     border-radius: 5%;
}
.payment-more{
  display:flex;
  justify-content: center;
  align-items: center;
  width:100%;

}
.payment{
  width:80%;
  height:40px;
  font-size:30rpx;

}
.payment-money{
  width:80%;
  height:40px;
  font-size:50rpx;
  font-weight: 700;
}
.payment-all{
  width:80%;
  height:40px;
}

.prompt{
  width:100%;
  height:50px;
  display:flex;
  justify-content: center;
  align-items: center;
}
.prompt-in{
  width:90%;
  font-size:20rpx;
  color:#565656;
}