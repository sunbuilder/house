page {
  background-color: #f1f1f1;
}

.inputRoom {
  width: 100vw;
  height: 16vw;
  border-top: 1px solid #cdcdcd;
  background-color: #f1f1f1;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: center;
  z-index: 20;
}

input {
  width: 76vw;
  height: 9.33vw;
  background-color: #fff;
  border-radius: 40rpx;
  margin-left: 2vw;
  padding: 0 3vw;
  font-size: 28rpx;
  color: #444;
}

.leftMsg {
  font-size: 35rpx;
  color: #444;
  line-height: 7vw;
  padding: 2vw 2.5vw;
  background-color: #fff;
  margin-left: -1.6vw;
  border-radius: 10rpx;
  z-index: 10;
}

.rightMsg {
  font-size: 35rpx;
  color: #444;
  line-height: 7vw;
  padding: 2vw 2.5vw;
  background-color: #96EB6A;
  margin-right: -1.6vw;
  border-radius: 10rpx;
  z-index: 10;
}
.button {
  border: 1px solid white;
  width: 18%;
  height: 80rpx;
  background: #0c0;
  color: white;
  line-height: 80rpx;
  margin-top: 10rpx;
  font-size: 28rpx;
}
.server-left{
  display: flex; 
  padding: 2vw 11vw 2vw 2vw;
}
.server-in{
   width: 11vw;
   height: 11vw;
}
.server-img{
  width: 11vw; 
  height: 11vw; 
  border-radius: 10rpx;
}
.server-right{
  display: flex; 
  justify-content: flex-end; 
  padding: 2vw 2vw 2vw 11vw;
}
.server-right-k{
  width: 4vw; 
  height: 11vw; 
  margin-right: 0.5vw; 
  display: flex; 
  align-items: center;
   z-index: 9
}
.server-left-k{
  width: 4vw; 
  height: 11vw; 
  margin-left: 0.5vw; 
  display: flex; 
  align-items: center; 
  z-index: 9;
}
