/* 没有记录 */
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.container image{
  height:150px;
  width:150px;
}
.container text{
  font-size:15px;
  color:#979797;
}
