/* 搜索 */
.search{
  width: 100%;
  height:40px;
   display:flex;
   justify-content: center;
   align-items: center;
   position:relative;    
   top:4px;  
}
.search-in{
  width:90%;
  height:40px;
  display: flex;
  align-items: center;  
  background-color:#e9e9e9; 
  border-radius: 2%;
  
}
  .search-in image {
    height:30px;
    width:30px;
    position:relative;    
    left:4px;
  }
  .search-in text {
    position:relative;    
    left:4px;
  }
  /* 楼房列表 */
.list-all{
  display:flex;
  align-items:center;
  justify-content:center;
  height:110px;
  width:100%;
  background-color:white;

}
.list-second{
  height:80px;
  width:90%;
  display:flex;
}
.list-second image{
  height:80px;
  width:35%;
  
}
.list-right{
  height:80px;
  width:65%;
}

.title-all{
  display:flex;
  justify-content:space-between;

}
.title-logn{  
  font-size:25rpx;
  height:15px;
  width:25px;
  color:white;
  background-color: #00C777;
  border-radius: 5rpx;
  display:flex;
  align-items:center;
  justify-content:center;
}
.title-title{
  font-size:30rpx;
  font-weight:600;
  position:relative;  
  left:10px;
}
.title{
  position:relative;  
  left:10px;
  display:flex;
}
/* 收藏 */
.collect image{
  height:20px;
  width:20px;

}
.list-right-text{
  font-size:25rpx;
  position:relative; 
  left:10px;
  color:#A4A9B3;

}
.list-right-in{
  position:relative;
  top:7px;    
  left:10px;
  width:95%;
  height: 7px;
  display:flex;
  align-items:center;
  justify-content:space-between;
  color:#FF0000;

}
.list-right-last{
  position:relative;
  top:16px;    
  left:10px;
  height:15px;
  width:32px;
  border:1px;
  border-style: solid;
}

/* 代理 */
