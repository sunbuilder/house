.details-all{
  width:100%;
  background-color:white;
}
.details-title{
   width:100%;
   height:70px;
   display:flex;
   align-items: center;
   justify-content: center;

}
.details-title-text{
  width:90%;
  height:50px;
  display:flex;
  align-items: center;
  font-size:35rpx;
}
.details-type{
   width:100%;
   height:40px;
   display:flex;
   align-items: center;
   justify-content: center;
}
.details-type-in{
   width:90%;
   display:flex;
   align-items: center;
}
.article-first{
   width:100%;
   display:flex;
   align-items: center;
   justify-content: center;
}
.article-first textarea{
  width:90%;
}
.article-img{
  width:100%;
   display:flex;
   align-items: center;
   justify-content: center;
   position:relative;    
   top:10px;
}
.article-img image{
  height:200px;
  width:90%;
}
.article-all{
  width:100%;
   display:flex;
   align-items: center;
   justify-content: center;
   position:relative;    
   top:20px;
}
.article-all textarea{
  width:90%;
}