.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}
/* 按钮 */
.btn {
  width: 80%;
  margin-top: 20rpx;
  background-color: yellow;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.btn::after {
  border-radius: 98rpx;
  border: 0; 
}
