/* pages/credit/report/salary/salary.wxss */
.point{
  height:20px;
  width:100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:25rpx;
  background:#FFEEE7;
  color:#FC5F1B;
}
/* 按钮 */
.btn {
  width: 80%;
  margin-top: 20rpx;
  background: yellow;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.btn::after {
  border-radius: 98rpx;
  border: 0; 
}