page{
 background-color:white;
}
/* 选择 */
#tabs{
   font-size: 28rpx;
   position:relative;    
   top:10px;
}
.tabs-box{
    display:flex;
    align-items:center;
    justify-content:center;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center
}
.tabs-item.selected {
    color:#15ADC5;
    border-bottom:2rpx solid #15ADC5;
}
.tabs-item {
    width: 25%;
    color:#666;
    border-bottom:1rpx solid #e9e9e9;
}
#tabs-all{
  width:100px;

}
/* 搜索 */
.search{
  width: 100%;
  height:40px;
   display:flex;
   justify-content: center;
   align-items: center;
   position:relative;    
   top:10px;  
}
.search-in{
  width:90%;
  height:40px;
  display: flex;
  align-items: center;  
  background-color:#e9e9e9; 
  border-radius: 2%;
  
}
  .search-in image {
    height:30px;
    width:30px;
    position:relative;    
    left:4px;
  }
  .input {
    position:relative;    
    left:4px;
  }
  /* 选择对应页面 */
  .ordert-detail{
   position:relative;    
   top:10px;

  }
/* 数据列表 */
  .list-all{
  display:flex;
  align-items:center;
  justify-content:center;
  height:120px;
  width:100%;

}
.list{
  height:90px;
  width:90%;
  display:flex;
  
}
.list image{
  height:100px;
  width:35%;
  
}
.list-right{
  height:100px;
  width:65%;
}
.title{
  position:relative;
  top:4px;    
  left:10px;
  font-size:35rpx;
  font-weight:600;
}
.list-right-text{
  font-size:30rpx;
  position:relative;
  top:15px;    
  left:10px;

}
.list-right-in{
  position:relative;
  top:30px;    
  left:10px;
  width:95%;
  height: 10px;
  display:flex;
  align-items:center;
  justify-content:space-between;

}

.list-right-in image{
  height:20px;
  width:20px;
  position:relative;
  top:5px; 
}

/* 弹框 */
