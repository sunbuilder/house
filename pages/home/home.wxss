page{
 background-color:white;
}

/* 搜索 */
.search{
  width: 100%;
  height:40px;
   display:flex;
   justify-content: center;
   align-items: center;
   position:relative;    
   top:4px;  
}
.search-in{
  width:90%;
  height:40px;
  display: flex;
  align-items: center;  
  background-color:#e9e9e9; 
  border-radius: 2%;
  
}
  .search-in image {
    height:30px;
    width:30px;
    position:relative;    
    left:4px;
  }
  .search-in text {
    position:relative;    
    left:4px;
  }
  /* 筛选 */
  .list{
   position:relative;    
   top:10px;
  height: 100%;
  width:100%;
  display:flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content:space-around;
  
}

.select{
  display: flex;
  justify-content: center;

}
 
.select_box {
    background-color: #e9e9e9;
    padding: 0 10rpx;
    width:95%;
    height:190px;
    position: absolute;
    top: 145px;
    z-index: 1;
    overflow: hidden;
    animation: myfirst 0.5s;
}
/* 选择 */
#tabs{
  font-size: 24rpx;
   position:relative;    
   top:10px;
}
.tabs-box{
    display:flex;
    align-items:center;
    justify-content:center;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center
}
.tabs-item.selected {
    color:#15ADC5;
    border-bottom:2rpx solid #15ADC5;
}
.tabs-item {
    width: 33.3%;
    color:#666;
    border-bottom:1rpx solid #e9e9e9;
}
.img-down{
  height:20rpx;
  width:20rpx;
  position:relative;  
  left:10rpx;

}
 /* 楼房列表 */
.list-all{
  display:flex;
  align-items:center;
  justify-content:center;
  height:100px;
  width:100%;
  background-color:white;

}
.list-second{
  height:80px;
  width:90%;
  display:flex;
  
}
.list-second image{
  height:80px;
  width:35%;
  
}
.list-right{
  height:80px;
  width:65%;
}

.title-all{
  display:flex;
  justify-content:space-between;

}
.title-logn{  
  font-size:25rpx;
  height:15px;
  width:25px;
  color:white;
  background-color: #00C777;
  border-radius: 5rpx;
  display:flex;
  align-items:center;
  justify-content:center;
}
.title-title{
  font-size:30rpx;
  font-weight:600;
  position:relative;  
  left:10px;
}
.title{
  position:relative;  
  left:10px;
  display:flex;
}
/* 收藏 */
.collect image{
  height:20px;
  width:20px;

}
.list-right-text{
  font-size:25rpx;
  position:relative; 
  left:10px;
  color:#A4A9B3;

}
.list-right-in{
  position:relative;
  top:7px;    
  left:10px;
  width:95%;
  height: 7px;
  display:flex;
  align-items:center;
  justify-content:space-between;
  color:#FF0000;

}
.list-right-last{
  position:relative;
  top:16px;    
  left:10px;
  height:15px;

}

/* 代理 */
.agent-all{
  height:15px;
  width: 100%;
  display:flex;
  background:white;
  align-items:center;
  justify-content:center;

}
.agent{
  height:15px;
  width: 90%;
  background-color: #FFF7F1;
  border-radius: 20rpx;
  display:flex;
  align-items:center;
  font-size:20rpx;
  color:#6A6880;

}
.agent image{
  position:relative;
  left:8px; 
}
.agent text{
  position:relative;
  left:10px; 
}
.deline{
  width:100%;
  height:5px;
 background:#EEEEEE;
}
/* 滑动 */
.scroll-header{
display: flex;
white-space: nowrap;
position:relative;    
top:45px;
}
/* 弹框 */
@import "../../wxPop/wxPop.wxss";
sup{
     font-size: 7px;
     vertical-align:text-top;
}
